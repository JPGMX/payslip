<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Annual salary</th>
    <th>Super rate</th>
    <th>Payment start date</th>
    <th>Gross income</th>
    <th>Income tax</th>
    <th>Net income</th>
    <th>Super</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.annual_salary %></td>
      <td><%= user.super_rate %></td>
      <td><%= user.payment_start_date %></td>
      <td><%= user.gross_income %></td>
      <td><%= user.income_tax %></td>
      <td><%= user.net_income %></td>
      <td><%= user.super %></td>
  <% end %>
    </tr>
  <%if @users	%>
   <tr> <td><%= link_to 'Export', users_index_path(format:"csv") %></td> </tr>
  <% end	  %>
  </tbody>
</table>

<%= render 'import', user: @user %>
